{
  "name": "react-animation-rating",
  "keywords": ["react", "rating", "animation"],
  "version": "1.0.0",
  "description": "React animation rating",
  "license": "ISC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "husky install",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-unknown --write **/*",
    "storybook": "start-storybook -p 6006 -s public --no-open",
    "build:storybook": "build-storybook",
    "build:scripts": "vite build",
    "build:types": "tsc --emitDeclarationOnly --project tsconfig.production.json && tsc-alias",
    "build": "run-p build:*"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-docs": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/addon-storysource": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/jest": "0.0.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/node-logger": "6.5.10",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@storybook/theming": "6.5.10",
    "@svgr/webpack": "6.3.1",
    "@trivago/prettier-plugin-sort-imports": "3.3.0",
    "@types/node": "14.0.1",
    "@types/react": "16.9.35",
    "@types/react-css-modules": "4.6.4",
    "@types/react-dom": "16.9.8",
    "@types/svgo": "2.6.4",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "@vitejs/plugin-react": "2.1.0",
    "autoprefixer": "10.4.8",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-perf": "3.3.1",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-css-order": "1.3.0",
    "sass": "1.54.9",
    "sass-loader": "13.0.2",
    "storybook-addon-designs": "6.3.1",
    "storybook-addon-pseudo-states": "1.15.1",
    "storybook-dark-mode": "1.1.0",
    "suppress-exit-code": "1.0.0",
    "tsc-alias": "1.7.0",
    "typescript": "4.6.4",
    "typescript-plugin-css-modules": "3.1.0",
    "vite": "3.1.0",
    "vite-plugin-svgr": "2.2.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "suppress-exit-code eslint --fix"
    ],
    "**/*": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "16.13.2",
    "npm": "8.1.2"
  }
}
